<resources>

    <!-- Name of the app -->
    <string name="app_name">Calls</string>

    <!-- AndroidManifest doesn't work with common_ strings, so use this workaround -->
    <skip />
    <string name="title_activity_settings" translatable="false">@string/common_settings</string>

    <!-- Title name of an unknown caller -->
    <string name="unknown">Unknown</string>

    <!-- Message shown when the user tries to open the Contacts app from the FAB button -->
    <string name="missing_app">You need to install the Contacts application.</string>

    <!-- Hint shown inside the text field when the user taps the Search button -->
    <string name="search_calls">Search calls</string>

    <!-- Shown on the main background when the call list is empty -->
    <string name="no_calls">No calls found.</string>

    <!-- Message shown when the user tries to use a disabled action for unknown/hidden numbers -->
    <string name="hidden_number">Action unavailable for hidden numbers.</string>

    <!-- Delete calls label for activity title, alert dialog title and settings button label.-->
    <string name="delete_calls">Delete calls</string>

    <!-- Warning when no calls are selected for deletion -->
    <string name="no_calls_selected">No calls selected.</string>

    <!-- Deletion result message -->
    <string name="calls_deleted">Deletion succeeded.</string>

    <!-- Deletion approval message for multiple calls. -->
    <string name="delete_calls_approval">Are you sure you want to delete the selected calls?</string>

    <!-- Deletion approval message for one call. -->
    <string name="delete_call_approval">Are you sure you want to delete the selected call?</string>

    <!-- Call date description separator -->
    <string name="day_info_older">Older</string>
</resources>
